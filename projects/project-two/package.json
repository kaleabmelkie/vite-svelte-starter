{
  "name": "project-two",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5002",
    "build": "vite build",
    "preview": "vite preview --port 4002",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest --passWithNoTests",
    "test:watch": "npm test -- --watch"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest",
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  },
  "dependencies": {
    "flowbite": "^1.5.1",
    "flowbite-svelte": "^0.22.5",
    "shared": "*",
    "svelte-navigator": "^3.1.6"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.9",
    "@sveltejs/vite-plugin-svelte": "^1.0.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/svelte": "^3.1.3",
    "@testing-library/user-event": "^14.3.0",
    "@tsconfig/svelte": "^3.0.0",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^28.1.3",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "cross-env": "^7.0.3",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-transform-stub": "^2.0.0",
    "postcss": "^8.4.14",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.6",
    "ts-jest": "^28.0.7",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  }
}
